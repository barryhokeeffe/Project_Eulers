   "source": [
    "def fib(N):\n",
    "\n",
    "    sum = 2\n",
    "\n",
    "    i = 1\n",
    "    j = 2\n",
    "\n",
    "    while i + j < N:\n",
    "\n",
    "        k = i + j \n",
    "\n",
    "        if k % 2 == 0:\n",
    "            sum += k\n",
    "\n",
    "        i = j \n",
    "        j = k\n",
    "\n",
    "    return sum"
   ]
  },
