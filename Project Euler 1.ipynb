    "def sum_TF(x):\n",
    "\n",
    "    sum = 0\n",
    "\n",
    "    for i in range(x):\n",
    "\n",
    "        if i % 3 == 0:\n",
    "            sum += i\n",
    "        elif i % 5 == 0:\n",
    "            sum += i\n",
    "\n",
    "    return sum"
